{
  "name": "@easyspace/aitable",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Next-generation Airtable-like component for EasySpace",
  "homepage": "https://github.com/easyspace/easyspace",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "source": "./src/index.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.js",
      "require": "./dist/store/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.js"
    },
    "./grid/error-handling": {
      "types": "./dist/grid/error-handling/index.d.ts",
      "import": "./dist/grid/error-handling/index.js",
      "require": "./dist/grid/error-handling/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "kanban"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:simple": "cross-env NODE_ENV=production rollup -c rollup.simple.config.js",
    "build:tsc": "cross-env NODE_ENV=production tsc",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./tsconfig.build.tsbuildinfo ./node_modules/.cache ./.eslintcache",
    "dev": "rollup -c --watch",
    "dev:tsc": "tsc --build tsconfig.build.json --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "dev:examples": "vite --config examples/vite.config.ts",
    "build:examples": "vite build --config examples/vite.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.1.0",
    "immer": "^10.0.4",
    "lodash": "npm:lodash-es@4.17.21",
    "lru-cache": "^10.2.0",
    "lucide-react": "^0.546.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-textarea-autosize": "^8.5.9",
    "react-use": "^17.6.0",
    "ts-keycode-enum": "^1.0.6",
    "use-file-picker": "^2.1.4"
  },
  "peerDependencies": {
    "@easyspace/sdk": "workspace:*",
    "@easyspace/ui": "workspace:*",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.2.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.18",
    "zustand": "^5.0.7"
  },
  "peerDependenciesMeta": {
    "@easyspace/ui": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "devDependencies": {
    "@easyspace/eslint-config": "workspace:*",
    "@easyspace/prettier-config": "workspace:*",
    "@easyspace/tailwind-config": "workspace:*",
    "@easyspace/typescript-config": "workspace:*",
    "@easyspace/ui": "workspace:*",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tailwindcss/postcss": "^4.1.15",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-hammerjs": "1.0.7",
    "@types/sharedb": "^3.3.10",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.21",
    "cross-env": "7.0.3",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.0",
    "rimraf": "5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.7.0",
    "vite": "^7.1.7",
    "vitest": "2.1.5",
    "zustand": "^5.0.7"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@easyspace/eslint-config/base",
      "@easyspace/eslint-config/react"
    ]
  },
  "prettier": "@easyspace/prettier-config"
}
